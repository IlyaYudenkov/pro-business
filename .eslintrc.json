{
  "extends": "next/core-web-vitals",
  "rules": {
    "react/react-in-jsx-scope": "off",
    "react/jsx-indent": [
      2,
      2
    ],
    "indent": [
      2,
      2
    ],
    "react/jsx-filename-extension": [
      2,
      {
